_terms = YES
cert mycert1 = example.com
cert mycert1 crt_path = /etc/ssl/mycert1.crt
cert mycert1 key_path = /etc/ssl/mycert1.key
cert mycert1 post_hook = systemctl reload nginx
cert mycert2 = sub.example.com
domain example.com = dns cloudflare MYTOKEN
domain sub.example.com = http /var/www/html
ip 203.0.113.1 = http /var/www/html
